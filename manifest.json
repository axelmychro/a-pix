{
  "manifest_version": 3,
  "name": "a-pix",
  "version": "1.0.0",
  "description": "Download X images on original quality.",
  "permissions": ["webRequest", "webRequestBlocking"],
  "host_permissions": ["*://pbs.twimg.com/*"],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "a-pix@axelmychro",
      "data_collection_permissions": {
        "required": ["none"]
      }
    }
  }
}
